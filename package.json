{
  "name": "typescript-slackbot-starter",
  "version": "1.0.0",
  "description": "This is starter template for slackbot built on typescript",
  "author": "Jungwoo-An (o1o9814@naver.com)",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ignore-pattern *.d.ts --ext .ts ./src/**/*",
    "dev": "cross-env NODE_ENV=development nodemon -e ts --exec \"ts-node -r tsconfig-paths/register ./src/index\"",
    "build": "cross-env NODE_ENV=development tsc",
    "start": "cross-env NODE_ENV=production node -r tsconfig-paths/register ./dist/src/index"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/lowdb": "^1.0.5",
    "@types/node": "^10.10.1",
    "eslint": "^5.6.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-typescript": "^0.12.0",
    "nodemon": "^1.18.4",
    "ts-node": "6",
    "tsconfig-paths": "^3.6.0",
    "typescript": "^3.0.3",
    "typescript-eslint-parser": "^18.0.0"
  },
  "dependencies": {
    "@slack/client": "^4.6.0",
    "consola": "^1.4.3",
    "cross-env": "^5.2.0"
  }
}
